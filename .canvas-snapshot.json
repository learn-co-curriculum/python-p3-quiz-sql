      "answers": [
        {
          "id": 5860,
          "text": "",
          "html": "<div class=\"highlight\"><pre class=\"highlight sql\"><code><span class=\"k\">ALTER</span> <span class=\"k\">TABLE</span> <span class=\"k\">table_name</span> <span class=\"k\">ADD</span> <span class=\"k\">COLUMN</span> <span class=\"k\">column_name</span> <span class=\"nb\">TEXT</span><span class=\"p\">;</span>\n</code></pre></div>",
